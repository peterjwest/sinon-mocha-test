{
  "include": [
    "src/**"
  ],
  "extension": [
    ".ts"
  ],
  "exclude": [
    "node_modules/**"
  ],
  "require": [
    "ts-node/register"
  ],
  "all": true,
  "check-coverage": true,
  "per-file": true,
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80,
  "watermarks": {
    "lines": [80, 95],
    "functions": [80, 95],
    "branches": [80, 95],
    "statements": [80, 95]
  },
  "reporter": [
    "text",
    "text-summary",
    "lcov"
  ]
}
