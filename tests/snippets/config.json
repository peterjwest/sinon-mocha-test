{
  "es-mocha": { "extension": "mjs", "command": ["mocha"] },
  "js-mocha": { "extension": "cjs", "command": ["mocha"] },
  "js-jest": { "extension": "cjs", "command": ["jest", "--config", "jest.config.json"] },
  "ts-jest": { "extension": "ts", "command": ["jest", "--config", "jest.config.json"] },
  "es-vite": { "extension": "mjs", "command": ["vitest", "run", "--config", "vite.config.ts"] },
  "ts-vite": { "extension": "ts", "command": ["vitest", "run", "--config", "vite.config.ts"] }
}
